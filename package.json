{
    "name": "wiki-spa-example",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "antd": "^3.1.3",
        "date-fns": "^1.29.0",
        "glamor": "^2.20.40",
        "prop-types": "^15.6.0",
        "ramda": "^0.25.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-markdown": "^3.1.4",
        "react-refetch": "^1.0.4",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.1.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom --testPathPattern '.*test.js$'",
        "test:acceptance":
            "react-scripts test --env=jsdom '.*acceptance.spec.js$'",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public",
        "fmt": "yarn fmt:js && yarn fmt:json",
        "fmt:js":
            "prettier --write '{*.js,.storybook/*.js,src/**/*.js}' && eslint --fix '{*.js,.storybook/*.js,src/**/*.js}'",
        "fmt:json": "prettier --parser json --write '{*.json,src/**/*.json}'"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.3.9",
        "@storybook/addon-knobs": "^3.3.9",
        "@storybook/addon-links": "^3.3.9",
        "@storybook/addon-storyshots": "^3.3.9",
        "@storybook/react": "^3.3.9",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.15.0",
        "eslint-plugin-flowtype": "^2.41.0",
        "eslint-plugin-react": "^7.5.1",
        "nightmare": "^2.10.0",
        "prettier": "^1.10.2",
        "react-test-renderer": "^16.2.0"
    }
}
